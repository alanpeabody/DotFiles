Rnavcommand factory spec/factories -glob=* -suffix=_factory.rb -default=model()
Rnavcommand enum app/enums -glob=* -suffix=.rb
Rnavcommand feature features -glob=**/* -suffix=.feature
Rnavcommand step features/step_definitions -glob=* -suffix=_steps.rb
Rnavcommand jmodel app/assets/javascripts/models -glob=* -suffix=.js
Rnavcommand jroute app/assets/javascripts/routes -glob=* -suffix=.js
Rnavcommand jcontroller app/assets/javascripts/controllers -glob=* -suffix=.js
Rnavcommand jview app/assets/javascripts/views -glob=* -suffix=.js
Rnavcommand jtemplate app/assets/javascripts/templates -glob=* -suffix=.hbs
